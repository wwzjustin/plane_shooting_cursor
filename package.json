  "license": "MIT",
  "devDependencies": {
    "vite": "^5.0.0",
    "gh-pages": "^6.0.0"
  },
  "dependencies": {
    "three": "^0.160.0"
  },
  "engines": {
    "node": ">=16.0.0"
  }
} 