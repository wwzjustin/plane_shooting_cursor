{
  "name": "3d-plane-game",
  "version": "1.0.0",
  "description": "A 3D aerial combat game built with Three.js featuring multi-weapon combat system and immersive audio",
  "main": "main.js",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
